{
  "index_patterns": ["clin-qa-analyses-*", "clin-staging-analyses-*"],
  "template": {
    "settings": {
      "number_of_shards": 1
    },
    "mappings": {
      "_source": {
        "enabled": true
      },
      "properties": {
        "securityTags": {
          "type": "keyword"
        },
        "cid": {
          "type": "keyword"
        },
        "timestamp": {
          "type": "date"
        }
      }
    }
  }
}